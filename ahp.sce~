Sheets  = readxls("./muestras/2/workbook.xls")
A = Sheets(1).value

// Se sumariza cada componente de las tripletas (RS)
RS = sum(A,2)

// Se sumariza cada columna de A (CS)
CS = sum(A,1)

A2 = cat(2, A(:,1) / CS(1), A(:,2) / CS(2), A(:,3) / CS(3), A(:,4) / CS(4))

EV = sum(A2,2)/4

LAMBDAMAX = CS * EV

CILAMBDAMAX